#custom makefile for Visual C++

@SET_MAKE@
AM_MAKEFLAGS = @AM_MAKEFLAGS@

srcdir = @srcdir@ 
top_srcdir = @top_srcdir@
VPATH = @srcdir@ 
incdir = @top_srcdir@
libdir = @prefix@/lib
includedir = @includedir@
prefix = @prefix@
datadir = @datadir@
datarootdir = @datarootdir@
install_sh = @install_sh@
mkdir_p = @mkdir_p@

top_builddir = ..
subdir = src

AM_CXXFLAGS = @AM_CXXFLAGS@
COPY_P = @COPY_P@
DIFF_Q = @DIFF_Q@
CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
CXX = @CXX@
CXXCPP = @CXXCPP@
CXXDEPMODE = @CXXDEPMODE@
CXXFLAGS = @CXXFLAGS@
CYGPATH_W = @CYGPATH_W@
OBJEXT = @OBJEXT@
PACKAGE = @PACKAGE@
RANLIB = @RANLIB@
SHELL = @SHELL@
SET_MAKE = @SET_MAKE@
DEFS = @DEFS@
LDFLAGS = @LDFLAGS@
DEPDIR = @DEPDIR@
INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@

CXXCOMPILE = $(CXX) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) \
	$(AM_CPPFLAGS) $(CPPFLAGS) $(AM_CXXFLAGS) $(CXXFLAGS)
CXXLINK = lib $(AM_LDFLAGS) $(LDFLAGS)
DEFAULT_INCLUDES = -I. -I$(srcdir) -I$(top_builddir)/HepMC
INCLUDES = -I$(top_builddir) -I$(top_srcdir) 
am__depfiles_maybe = depfiles
depcomp = $(SHELL) $(top_srcdir)/depcomp

NORMAL_INSTALL = :
POST_INSTALL = :
NORMAL_UNINSTALL = :
mkinstalldirs = $(install_sh) -d
am__strip_dir = `echo $$p | sed -e 's|^.*/||'`;
am__installdirs = "$(DESTDIR)$(libdir)"

SOURCES = CompareGenEvent.cc \
	  Flow.cc \
	  GenEvent.cc \
   	  GenEventStreamIO.cc	\
	  GenParticle.cc \
	  GenCrossSection.cc	\
	  GenVertex.cc \
	  GenRanges.cc \
	  HeavyIon.cc	\
	  IO_AsciiParticles.cc \
	  IO_GenEvent.cc \
	  PdfInfo.cc	\
	  Polarization.cc \
	  SearchVector.cc \
	  StreamHelpers.cc	\
	  StreamInfo.cc	\
          Units.cc	\
	  WeightContainer.cc

OBJ = CompareGenEvent.obj \
      Flow.obj \
      GenEvent.obj \
      GenEventStreamIO.obj	\
      GenParticle.obj \
      GenCrossSection.obj \
      GenVertex.obj \
      GenRanges.obj \
      HeavyIon.obj	\
      IO_AsciiParticles.obj \
      IO_GenEvent.obj	\
      PdfInfo.obj	\
      Polarization.obj \
      SearchVector.obj \
      StreamHelpers.obj	\
      StreamInfo.obj	\
      Units.obj	\
      WeightContainer.obj


lib_LIBRARIES = HepMC.lib
LIBRARIES = $(lib_LIBRARIES)
libLIBRARIES_INSTALL = $(INSTALL_DATA)

all: all-am
check: check-am
check-am: all-am
all-am: VCMakefile $(LIBRARIES)

install: install-am
install-exec: install-exec-am
uninstall: uninstall-am

install-am: all-am
	@$(MAKE) $(AM_MAKEFLAGS) install-exec-am

install-exec-am: install-libLIBRARIES

uninstall-am: uninstall-LIBRARIES

install-libLIBRARIES: $(lib_LIBRARIES)
	@$(NORMAL_INSTALL)
	test -z "$(libdir)" || $(mkdir_p) "$(DESTDIR)$(libdir)"
	@list='$(lib_LIBRARIES)'; for p in $$list; do \
	  if test -f $$p; then \
	    f=$(am__strip_dir) \
	    echo " $(libLIBRARIES_INSTALL) '$$p' '$(DESTDIR)$(libdir)/$$f'"; \
	    $(libLIBRARIES_INSTALL) "$$p" "$(DESTDIR)$(libdir)/$$f"; \
	  else :; fi; \
	done
	@$(POST_INSTALL)
	@list='$(lib_LIBRARIES)'; for p in $$list; do \
	  if test -f $$p; then \
	    p=$(am__strip_dir) \
	    echo " $(RANLIB) '$(DESTDIR)$(libdir)/$$p'"; \
	    $(RANLIB) "$(DESTDIR)$(libdir)/$$p"; \
	  else :; fi; \
	done

uninstall-libLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@list='$(lib_LIBRARIES)'; for p in $$list; do \
	  p=$(am__strip_dir) \
	  echo " rm -f '$(DESTDIR)$(libdir)/$$p'"; \
	  rm -f "$(DESTDIR)$(libdir)/$$p"; \
	done

clean-libLIBRARIES:
	-test -z "$(lib_LIBRARIES)" || rm -f $(lib_LIBRARIES)

HepMC.lib: $(OBJ)
	$(CXXLINK) /out:`$(CYGPATH_W) '$@'` $(OBJ)

clean:  clean-am
clean-am:  clean-LIBRARIES mostlyclean

clean-LIBRARIES:
	-test -z "$(LIBRARIES)" || rm -f $(LIBRARIES)

mostlyclean: 
	-rm -f *.$(OBJEXT)

.SUFFIXES:
.SUFFIXES: .cc .obj

.PRECIOUS: VCMakefile
VCMakefile: @srcdir@/VCMakefile.in $(top_builddir)/config.status
	@case '$?' in \
	  *config.status*) \
	    cd $(top_builddir) && $(MAKE) $(AM_MAKEFLAGS) am--refresh;; \
	  *) \
	    echo ' cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@ $(am__depfiles_maybe)'; \
	    cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@ $(am__depfiles_maybe);; \
	esac;

##.cc.obj:
##	cl /EHsc /I`$(CYGPATH_W) '$(incdir)'` /Fo$@ /c `$(CYGPATH_W) '$<'`

.cc.obj:
	source='$<' object='$@' libtool=no \
	DEPDIR=$(DEPDIR) $(CXXDEPMODE) $(depcomp) \
	$(CXXCOMPILE) /c /Fo$@ `$(CYGPATH_W) '$<'`

.PHONY: all all-am check check-am clean clean-am distclean

.NOEXPORT:
