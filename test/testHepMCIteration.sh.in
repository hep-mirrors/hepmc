#! /bin/sh
# @configure_input@

./testHepMCIteration@EXEEXT@ >& testHepMCIteration.cout

OS=`uname`
case "$OS" in
CYGWIN*)
  #don't compare these on Windows (hopelessly different default output)
  cmd1=
  ;;
Darwin*)
  # MacOSX fix
  cmd1=`sed 's/(-0,0)/(0,0)/g'  testHepMCIteration.out  | \
  	@DIFF_Q@ - @srcdir@/testHepMCIteration.dat`
  ;;
*)
  cmd1=`sed 's/(-0,0)/(0,0)/g'  testHepMCIteration.out  | \
  	@DIFF_Q@ - @srcdir@/testHepMCIteration.dat`
esac

if [ -n "$cmd1" ]
then
  echo $cmd1
  exit 1;
fi

exit 0;
