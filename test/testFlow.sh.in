#! /bin/bash
# @configure_input@

rm -f testFlow.out 
./testFlow@EXEEXT@

if [ "$?" -ne 0 ]
then
  exit 1;
fi

if [ ! -e testFlow.out ]
then
   echo "ERROR: testFlow.out does not exist"
   exit 1;
fi

OS=`uname`
case "$OS" in
CYGWIN*)
if [ "@HEPMC_DEFAULT_MOM_UNIT@" = "MEV" ];then
  cmd1=`sed 's/e+0/e+/g'  testFlow.out  | \
     sed 's/e-0/e-/g'  -  | sed 's/MEV/GEV/g' - | \
	@DIFF_Q@ - @srcdir@/testFlow.output`
else
  cmd1=`sed 's/e+0/e+/g'  testFlow.out  | \
     sed 's/e-0/e-/g'  -  | \
	@DIFF_Q@ - @srcdir@/testFlow.output`
fi
  cmd2=`@DIFF_Q@ testFlow.out4 testFlow.out5`
  ;;
*)
if [ "@HEPMC_DEFAULT_MOM_UNIT@" = "MEV" ];then
  cmd1=`sed 's/MEV/GEV/g' testFlow.out | @DIFF_Q@ - @srcdir@/testFlow.output`
else
  cmd1=`@DIFF_Q@ testFlow.out @srcdir@/testFlow.output`
fi
  cmd2=`@DIFF_Q@ testFlow.out4 testFlow.out5`
esac

if [ -n "$cmd1" ]
then
  echo $cmd1
  exit 1;
fi

if [ -n "$cmd2" ]
then
  echo $cmd2
  exit 1;
fi

exit 0;
