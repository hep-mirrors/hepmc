#! /bin/sh
# @configure_input@

# want to see this output
./testMass@EXEEXT@

OS=`uname`
case "$OS" in
CYGWIN*)
  cmd1=`sed 's/e+0/e+/g'  testMass1.dat  | \
     sed 's/e-0/e-/g'  -  | \
	@DIFF_Q@ - @srcdir@/testMass1.dat`
  ;;
*)
  cmd1=`@DIFF_Q@ testMass1.dat @srcdir@/testMass1.dat`
esac

if [ -n "$cmd1" ]
then
  echo $cmd1
  exit 1;
fi

if ( ! `@DIFF_Q@ testMass2.dat testMass1.dat` )
then
  echo "comparison of testMass2.dat failed"
  exit 1;
fi

exit 0;
