#! /bin/sh
# @configure_input@

./testMass@EXEEXT@

OS=`uname`
case "$OS" in
CYGWIN*)
  sed 's/e+0/e+/g'  testMass1.dat  | \
     sed 's/e-0/e-/g'  -  | \
	@DIFF_Q@ - @srcdir@/testMass1.dat
  @DIFF_Q@ testMass2.dat testMass1.dat
  ;;
Darwin*)
  # MacOSX fix
  @DIFF_Q@ testMass1.dat @srcdir@/testMass1.dat
  @DIFF_Q@ testMass2.dat testMass1.dat
  ;;
*)
  @DIFF_Q@ testMass1.dat @srcdir@/testMass1.dat
  @DIFF_Q@ testMass2.dat testMass1.dat
esac
