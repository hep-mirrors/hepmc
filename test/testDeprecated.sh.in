#! /bin/sh
# @configure_input@

./testDeprecated@EXEEXT@ 

OS=`uname`
case "$OS" in
CYGWIN*)
  #don't compare these on Windows (hopelessly different default output)
  sed 's/e+0/e+/g' testAscii.dat   | \
     sed 's/e-0/e-/g' -   | \
	@DIFF_Q@ - @srcdir@/testAscii.dat
  sed 's/e+0/e+/g'  testHepMCExtended.dat  | \
     sed 's/e-0/e-/g'  -  | \
	@DIFF_Q@ - @srcdir@/testHepMCExtended.dat
  ;;
Darwin*)
  # MacOSX fix
  @DIFF_Q@ testAscii.dat @srcdir@/testAscii.dat
  @DIFF_Q@ testHepMCExtended.dat @srcdir@/testHepMCExtended.dat
  ;;
*)
  @DIFF_Q@ testAscii.dat @srcdir@/testAscii.dat
  @DIFF_Q@ testHepMCExtended.dat @srcdir@/testHepMCExtended.dat
esac
